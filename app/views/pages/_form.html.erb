<div summary="Page form fields" >
  <div class="form-group" >
    <%= f.label(:subject_id) %>    <%= f.text_field(:subject_id ,:class=>"form-control")  %>
  </div>
  <div class="form-group">
    <%= f.label(:name) %>    <%= f.text_field(:name,:class=>"form-control") %>
  </div>
  <div class="form-group">
    <%= f.label(:permalink) %>    <%= f.text_field(:permalink,:class=>"form-control") %>
  </div>
  <div class="form-group">
    <%= f.select(:position,1..@page_count) %>    <%= f.text_field(:position,:class=>"form-control") %>
  </div>
  <div class="form-group">
    <%= f.label(:visible) %>
    <%= f.check_box(:visible,:class=>"form-control") %>
  </div>
</div>